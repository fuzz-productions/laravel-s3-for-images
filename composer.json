{
	"name": "fuzz/s3-for-images",
	"description": "A wrapper around the Laravel filesystem to help with image uploads.",
	"authors": [
		{
			"name": "Fuzz Productions",
			"email": "web@fuzzproductions.com"
		}
	],
	"require": {
		"laravel/framework": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.*",
		"league/flysystem-aws-s3-v3": "1.0.6"
	},
	"require-dev": {
		"orchestra/testbench": "3.2.*",
		"mockery/mockery": "0.9.*",
		"phpunit/phpunit": "~4.0"
	},
	"autoload": {
		"psr-4": {
			"Fuzz\\S3ForImages\\": "src/",
			"Fuzz\\S3ForImages\\Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev"
}
